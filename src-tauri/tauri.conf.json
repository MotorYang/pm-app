{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ProManager",
  "version": "1.0.0",
  "identifier": "github.motoryang.pro-manager",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "项目管理",
        "width": 1250,
        "height": 800,
        "decorations": false,
        "resizable": true,
        "transparent": false,
        "minWidth": 1024,
        "minHeight": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["nsis", "deb", "dmg", "updater"],
    "icon": [
      "icons/icon.png",
      "icons/icon.ico",
      "icons/icon.icns"
    ],
    "resources": []
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": ["https://motoryang.github.io/pm-app/update.json"],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDQxNTRCQTI0MUM1NDhFN0YKUldSL2psUWNKTHBVUVZHaU1QWFdoUDJrVjBJb2J0WkJ2ekVtMkVqSUNhc1hsVzhYd1F5aVJXUlQK"
    }
  }
}
